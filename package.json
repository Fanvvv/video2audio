{
    "name": "video2audio",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "description": "Video to Audio",
    "author": "Fan",
    "license": "MIT",
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "main": "dist-electron/main/index.js",
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build && electron-builder",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.21.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "electron": "^29.1.1",
        "electron-builder": "^24.13.3",
        "eslint": "^8.57.0",
        "typescript": "^5.4.2",
        "unocss": "^0.61.0",
        "vite": "^5.1.5",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vue": "^3.4.21",
        "vue-tsc": "^2.0.6"
    },
    "dependencies": {
        "ffmpeg-static": "^5.2.0",
        "fluent-ffmpeg": "^2.1.3"
    }
}
